{
  "name": "iv-openapi",
  "version": "1.0.0",
  "type": "module",
  "author": "ChunkyProgrammer",
  "license": "MIT",
  "devDependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "eslint": "^8.55.0",
    "eslint-plugin-yml": "^1.10.0",
    "npm-run-all2": "^6.1.1",
    "openapi-types": "^7.0.0",
    "yaml": "^2.3.4",
    "yaml-eslint-parser": "^1.2.2"
  },
  "scripts": {
    "build": "run-p update-servers lint validate create-json-swagger copy-json-swagger",
    "lint": "eslint --ext .yml,.yaml ./openapi/",
    "validate": "node ./openapi/scripts/validate-swagger.js",
    "update-servers": "node ./openapi/scripts/update-servers.js",
    "build-clients": "run-s build-client-*",
    "build-client-ruby": "openapi-generator-cli generate -g ruby -i ./openapi/swagger.yaml -o ./openapi/dist/ruby-client",
    "build-client-typescript": "openapi-generator-cli generate -g typescript-axios -i ./openapi/swagger.yaml -o ./openapi/dist/ts-client",
    "build-client-csharp": "openapi-generator-cli generate -g csharp -i ./openapi/swagger.yaml -o ./openapi/dist/csharp-client",
    "build-client-python": "openapi-generator-cli generate -g python -i ./openapi/swagger.yaml -o ./openapi/dist/python-client",
    "build-client-kotlin": "openapi-generator-cli generate -g kotlin -i ./openapi/swagger.yaml -o ./openapi/dist/kotlin-client",
    "build-client-dart": "openapi-generator-cli generate -g dart -i ./openapi/swagger.yaml -o ./openapi/dist/dart-client",
    "build-client-rust": "openapi-generator-cli generate -g rust -i ./openapi/swagger.yaml -o ./openapi/dist/rust-client",
    "build-client-swift": "openapi-generator-cli generate -g swift5 -i ./openapi/swagger.yaml -o ./openapi/dist/swift5-client",
    "build-client-go": "openapi-generator-cli generate -g go -i ./openapi/swagger.yaml -o ./openapi/dist/go-client",
    "build-client-crystal": "openapi-generator-cli generate -g crystal -i ./openapi/swagger.yaml -o ./openapi/dist/crystal-client",
    "copy-json-swagger": "node ./openapi/scripts/copy-open-api-json.js",
    "create-json-swagger": "openapi-generator-cli generate -g openapi -i ./openapi/swagger.yaml -o ./openapi/docs/json"
  }
}
